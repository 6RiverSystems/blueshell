{
	"compileOnSave": true,
	"compilerOptions": {
		"outDir": "../dist-test",
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowJs": true,
		"sourceMap": true,
		"declaration": false,
		"forceConsistentCasingInFileNames": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"noImplicitReturns": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"bootstrap.js"
	]
}
