{
	"compileOnSave": true,
	"compilerOptions": {
		"declaration": false,
		"outDir": "../dist/test",
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowJs": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"noImplicitAny": true,
		"noImplicitReturns": true
	},
	"exclude": [
	]
}
