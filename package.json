{
  "name": "blueshell",
  "version": "1.0.0-beta.2",
  "description": "A Behavior Tree implementation in modern Javascript",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm-run-all pretest:eslint",
    "pretest:eslint": "eslint .",
    "clean": "shx rm -rf dist",
    "build": "tsc",
    "test": "npm-run-all clean build test:mocha",
    "test:mocha": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ istanbul cover node_modules/.bin/_mocha -- ${MOCHA_OPTS} dist/test",
    "test:no-cover": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ mocha dist/test",
    "test:debug": "npm run test:no-cover -- --debug-brk dist/test",
    "posttest": "nsp check",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/6RiverSystems/blueshell.git"
  },
  "keywords": [
    "behavior-tree",
    "finite",
    "state",
    "machines"
  ],
  "author": "Joshua Chaitin-Pollak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/6RiverSystems/blueshell/issues"
  },
  "homepage": "https://github.com/6RiverSystems/blueshell#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "3.3.1",
    "eslint-config-defaults": "~9.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-junit-reporter": "1.12.0",
    "nsp": "^2.2.1",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "npm-run-all": "^3.0.0",
    "shx": "^0.1.4",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10"
  }
}
