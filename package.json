{
  "name": "blueshell",
  "version": "2.0.0-alpha.1",
  "description": "A Behavior Tree implementation in modern Javascript",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm-run-all pretest:eslint",
    "pretest:eslint": "shx echo eslint .",
    "clean": "npm-run-all clean:js clean:test",
    "clean:js": "shx rm -rf dist",
    "clean:test": "shx rm -rf dist-test",
    "build": "npm-run-all build:js",
    "build:js": "tsc",
    "build:test": "tsc --project ./test --pretty",
    "test": "npm-run-all clean build:js build:test test:mocha",
    "test:mocha": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ istanbul cover node_modules/.bin/_mocha -- ${MOCHA_OPTS} dist-test",
    "test:no-cover": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ mocha dist-test",
    "test:debug": "npm run test:no-cover -- --debug-brk dist-test",
    "posttest": "nsp check",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/6RiverSystems/blueshell.git"
  },
  "keywords": [
    "behavior-tree",
    "finite",
    "state",
    "machines"
  ],
  "author": "Joshua Chaitin-Pollak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/6RiverSystems/blueshell/issues"
  },
  "homepage": "https://github.com/6RiverSystems/blueshell#readme",
  "devDependencies": {
    "@types/chai": "3.4.32",
    "@types/mocha": "^2.2.31",
    "chai": "3.5.0",
    "eslint": "3.4.0",
    "eslint-config-defaults": "~9.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "mocha-junit-reporter": "1.12.0",
    "npm-run-all": "3.0.0",
    "nsp": "2.6.1",
    "proxyquire": "1.7.10",
    "shx": "0.1.4",
    "sinon": "1.17.5",
    "source-map-support": "^0.4.2",
    "tslint": "3.15.1",
    "typescript": "^2.0.2"
  },
  "dependencies": {
    "@types/archy": "0.0.29",
    "archy": "1.0.0"
  }
}
