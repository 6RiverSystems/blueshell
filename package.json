{
  "name": "blueshell",
  "version": "0.0.0-development",
  "description": "A Behavior Tree implementation in modern Javascript",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepare": "tsc --pretty",
    "pretest": "npm-run-all pretest:eslint",
    "pretest:eslint": "shx echo eslint .",
    "clean": "shx rm -rf dist",
    "build": "tsc --pretty",
    "test": "NODE_ENV=test nyc mocha",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "posttest": "nsp check",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/6RiverSystems/blueshell.git"
  },
  "keywords": [
    "behavior-tree",
    "finite",
    "state",
    "machines"
  ],
  "author": "Joshua Chaitin-Pollak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/6RiverSystems/blueshell/issues"
  },
  "homepage": "https://github.com/6RiverSystems/blueshell#readme",
  "devDependencies": {
    "@types/chai": "4.0.6",
    "@types/mocha": "2.2.44",
    "@types/uuid": "3.4.3",
    "@types/archy": "0.0.31",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-defaults": "~9.0.0",
    "husky": "^0.13.4",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-lcov-reporter": "1.3.0",
    "npm-run-all": "4.1.2",
    "nsp": "^2.2.1",
    "nyc": "11.3.0",
    "proxyquire": "1.8.0",
    "shx": "0.2.2",
    "sinon": "^2.2.0",
    "source-map-support": "0.5.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.6.2",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "archy": "^1.0.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "chore"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "Follow <type>(<scope>): <subject> pattern, max 100 chars.",
      "autoFix": false
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
