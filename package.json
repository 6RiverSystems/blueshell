{
  "name": "blueshell",
  "version": "0.0.0-development",
  "description": "A Behavior Tree implementation in modern Javascript",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ istanbul cover node_modules/.bin/_mocha -- ${MOCHA_OPTS}",
    "test:no-cover": "NODE_ENV=test NODE_PATH=$NODE_PATH:./ mocha",
    "test:debug": "npm run test:no-cover -- --debug-brk",
    "posttest": "nsp check",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/6RiverSystems/blueshell.git"
  },
  "keywords": [
    "behavior-tree",
    "finite",
    "state",
    "machines"
  ],
  "author": "Joshua Chaitin-Pollak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/6RiverSystems/blueshell/issues"
  },
  "homepage": "https://github.com/6RiverSystems/blueshell#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-config-defaults": "~9.0.0",
    "husky": "^0.13.4",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-junit-reporter": "^1.13.0",
    "nsp": "^2.2.1",
    "proxyquire": "^1.7.3",
    "sinon": "^2.2.0",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "archy": "^1.0.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "chore"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "Follow <type>(<scope>): <subject> pattern, max 100 chars.",
      "autoFix": false
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
